{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist", "public/pagefind"],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "@lib/*": ["lib/*"],
      "@components/*": ["components/*"],
      "@layouts/*": ["layouts/*"],
      "@styles/*": ["styles/*"],
      "@utils/*": ["utils/*"],
      "@pages/*": ["pages/*"],
      "@assets/*": ["assets/*"],
      "@locales/*": ["locales/*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}
